{
  "description": "This sample demonstrates filtering null or empty values from data for data quality.",
  "model": {
    "inputObjects": [
      {
        "jsonString": "{\r\n  \"contacts\": [\r\n    {\"name\": \"Alice\", \"email\": \"alice@example.com\"},\r\n    {\"name\": \"Bob\", \"email\": null},\r\n    {\"name\": \"Charlie\", \"email\": \"\"},\r\n    {\"name\": \"David\", \"email\": \"david@example.com\"}\r\n  ]\r\n}",
        "name": "data"
      }
    ],
    "name": "Sample-137",
    "OutputObjects": [
      {
        "jsonString": "{\n  \"contacts\": [\n    {\"name\": \"Alice\", \"email\": \"alice@example.com\"},\n    {\"name\": \"David\", \"email\": \"david@example.com\"}\n  ]\n}",
        "name": "data"
      }
    ],
    "scriptText": "[\r\n  {\r\n    \"path\": \"$.data.contacts[*]\",\r\n    \"condition\": \"@.email != null && @.email != ''\",\r\n    \"command\": \"filter\"\r\n  }\r\n]"
  },
  "tags": [
    "filter",
    "etl"
  ],
  "title": "Filter out null and empty emails"
}