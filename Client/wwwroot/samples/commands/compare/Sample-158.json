  "scriptText": "[\r\n  {\r\n    \"leftPath\": \"$.original\",\r\n    \"rightPath\": \"$.updated\",\r\n    \"targetPath\": \"$.diff\",\r\n    \"command\": \"compare\"\r\n  }\r\n]",
