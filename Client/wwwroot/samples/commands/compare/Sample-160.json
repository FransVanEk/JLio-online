{
  "title": "Compare nested objects",
  "tags": ["compare"],
  "inputObjects": [
    {
      "v1": {
        "user": {
          "profile": {
            "email": "old@example.com"
          }
        }
      },
      "v2": {
        "user": {
          "profile": {
            "email": "new@example.com"
          }
        }
      }
    }
  ],
  "scriptText": "[\r\n  {\r\n    \"leftPath\": \"$.v1\",\r\n    \"rightPath\": \"$.v2\",\r\n    \"targetPath\": \"$.delta\",\r\n    \"command\": \"compare\"\r\n  }\r\n]",
  "outputObjects": [
    {
      "v1": {
        "user": {
          "profile": {
            "email": "old@example.com"
          }
        }
      },
      "v2": {
        "user": {
          "profile": {
            "email": "new@example.com"
          }
        }
      },
      "delta": {
        "user.profile.email": {
          "left": "old@example.com",
          "right": "new@example.com"
        }
      }
    }
  ]
}
