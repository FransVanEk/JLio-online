{
  "title": "Compare configuration versions",
  "tags": [
    "compare"
  ],
  "model": {
    "name": "Sample-162",
    "scriptText": "[\r\n  {\r\n    \"leftPath\": \"$.configOld\",\r\n    \"rightPath\": \"$.configNew\",\r\n    \"targetPath\": \"$.configDiff\",\r\n    \"command\": \"compare\"\r\n  }\r\n]",
    "inputObjects": [
      {
        "configOld": {
          "database": {
            "host": "localhost",
            "port": 5432
          },
          "cache": {
            "enabled": false
          }
        },
        "configNew": {
          "database": {
            "host": "db.server.com",
            "port": 5432
          },
          "cache": {
            "enabled": true
          }
        }
      }
    ],
    "outputObjects": [
      {
        "configOld": {
          "database": {
            "host": "localhost",
            "port": 5432
          },
          "cache": {
            "enabled": false
          }
        },
        "configNew": {
          "database": {
            "host": "db.server.com",
            "port": 5432
          },
          "cache": {
            "enabled": true
          }
        },
        "configDiff": {
          "database.host": {
            "left": "localhost",
            "right": "db.server.com"
          },
          "cache.enabled": {
            "left": false,
            "right": true
          }
        }
      }
    ]
  },
  "description": "This sample shows compare configuration versions."
}