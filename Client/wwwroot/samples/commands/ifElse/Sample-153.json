{
    "title":  "IfElse based on value check",
    "tags":  [
                 "ifElse"
             ],
    "model":  {
                  "name":  "Sample-153",
                  "scriptText":  "[\r\n  {\r\n    \"path\": \"$.user\",\r\n    \"condition\": \"@.age \u003e= 18\",\r\n    \"ifTrue\": [\r\n      {\r\n        \"path\": \"$.user.status\",\r\n        \"value\": \"adult\",\r\n        \"command\": \"set\"\r\n      }\r\n    ],\r\n    \"ifFalse\": [\r\n      {\r\n        \"path\": \"$.user.status\",\r\n        \"value\": \"minor\",\r\n        \"command\": \"set\"\r\n      }\r\n    ],\r\n    \"command\": \"ifElse\"\r\n  }\r\n]",
                  "inputObjects":  [
                                       {
                                           "jsonString":  "{\"user\":{\"age\":25,\"status\":\"\"}}",
                                           "name":  "data"
                                       }
                                   ],
                  "outputObjects":  [
                                        {
                                            "jsonString":  "{\"user\":{\"age\":25,\"status\":\"adult\"}}",
                                            "name":  "data"
                                        }
                                    ]
              },
    "description":  "This sample shows ifelse based on value check."
}
