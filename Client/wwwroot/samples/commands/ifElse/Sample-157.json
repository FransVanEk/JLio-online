{
    "title":  "IfElse for access control",
    "tags":  [
                 "ifElse"
             ],
    "model":  {
                  "name":  "Sample-157",
                  "scriptText":  "[\r\n  {\r\n    \"path\": \"$.user\",\r\n    \"condition\": \"@.role == \u0027admin\u0027\",\r\n    \"ifTrue\": [\r\n      {\r\n        \"path\": \"$.user.permissions\",\r\n        \"value\": [\"read\", \"write\", \"delete\"],\r\n        \"command\": \"set\"\r\n      }\r\n    ],\r\n    \"ifFalse\": [\r\n      {\r\n        \"path\": \"$.user.permissions\",\r\n        \"value\": [\"read\"],\r\n        \"command\": \"set\"\r\n      }\r\n    ],\r\n    \"command\": \"ifElse\"\r\n  }\r\n]",
                  "inputObjects":  [
                                       {
                                           "jsonString":  "{\\\"user\\\":{\\\"role\\\":\\\"admin\\\",\\\"permissions\\\":[]}}",
                                           "name":  "u"
                                       }
                                   ],
                  "outputObjects":  [
                                        {
                                            "jsonString":  "{\\\"user\\\":{\\\"role\\\":\\\"admin\\\",\\\"permissions\\\":[\\\"read\\\",\\\"write\\\",\\\"delete\\\"]}}",
                                            "name":  "u"
                                        }
                                    ]
              },
    "description":  "This sample shows ifelse for access control."
}
