{
    "title":  "Flatten specific path only",
    "tags":  [
                 "flatten"
             ],
    "model":  {
                  "name":  "Sample-135",
                  "scriptText":  "[\r\n  {\r\n    \"path\": \"$.company.employee\",\r\n    \"delimiter\": \".\",\r\n    \"command\": \"flatten\"\r\n  }\r\n]",
                  "inputObjects":  [
                                       {
                                           "jsonString":  "{\"company\":{\"name\":\"Acme Corp\",\"employee\":{\"id\":123,\"details\":{\"role\":\"Developer\",\"level\":\"Senior\"}}}}",
                                           "name":  "data"
                                       }
                                   ],
                  "outputObjects":  [
                                        {
                                            "jsonString":  "{\"company\":{\"name\":\"Acme Corp\",\"employee.id\":123,\"employee.details.role\":\"Developer\",\"employee.details.level\":\"Senior\"}}",
                                            "name":  "data"
                                        }
                                    ]
              },
    "description":  "This sample shows flatten specific path only."
}
