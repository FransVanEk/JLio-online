{
    "title":  "Flatten array elements",
    "tags":  [
                 "flatten"
             ],
    "model":  {
                  "name":  "Sample-136",
                  "scriptText":  "[\r\n  {\r\n    \"path\": \"$.items[*]\",\r\n    \"delimiter\": \"_\",\r\n    \"command\": \"flatten\"\r\n  }\r\n]",
                  "inputObjects":  [
                                       {
                                           "jsonString":  "{\"items\":[{\"product\":{\"name\":\"Phone\",\"specs\":{\"memory\":\"128GB\"}}}]}",
                                           "name":  "data"
                                       }
                                   ],
                  "outputObjects":  [
                                        {
                                            "jsonString":  "{\"items\":[{\"product_name\":\"Phone\",\"product_specs_memory\":\"128GB\"}]}",
                                            "name":  "data"
                                        }
                                    ]
              },
    "description":  "This sample shows flatten array elements."
}
