{
  "description": "This sample demonstrates grouping with filtering to create targeted data segments.",
  "model": {
    "inputObjects": [
      {
        "jsonString": "{\r\n  \"employees\": [\r\n    {\"name\": \"Alice\", \"department\": \"Engineering\", \"salary\": 80000},\r\n    {\"name\": \"Bob\", \"department\": \"Sales\", \"salary\": 60000},\r\n    {\"name\": \"Charlie\", \"department\": \"Engineering\", \"salary\": 95000},\r\n    {\"name\": \"David\", \"department\": \"Sales\", \"salary\": 55000}\r\n  ]\r\n}",
        "name": "company"
      }
    ],
    "name": "Sample-147",
    "OutputObjects": [
      {
        "jsonString": "{\n  \"highEarners\": {\n    \"Engineering\": [\n      {\"name\": \"Alice\", \"department\": \"Engineering\", \"salary\": 80000},\n      {\"name\": \"Charlie\", \"department\": \"Engineering\", \"salary\": 95000}\n    ]\n  }\n}",
        "name": "company"
      }
    ],
    "scriptText": "[\r\n  {\r\n    \"path\": \"$.company.filtered\",\r\n    \"source\": \"$.company.employees\",\r\n    \"condition\": \"@.salary > 70000\",\r\n    \"command\": \"filter\"\r\n  },\r\n  {\r\n    \"path\": \"$.company.highEarners\",\r\n    \"source\": \"$.company.filtered\",\r\n    \"groupBy\": \"department\",\r\n    \"command\": \"group\"\r\n  }\r\n]"
  },
  "tags": [
    "group",
    "filter",
    "etl"
  ],
  "title": "Filter and group high earners"
}