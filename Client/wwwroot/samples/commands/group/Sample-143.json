    "name": "Sample-143",
    "OutputObjects": [
      {
        "jsonString": "{\n  \"grouped\": {\n    \"Electronics\": [\n      {\"name\": \"Laptop\", \"category\": \"Electronics\", \"price\": 1000},\n      {\"name\": \"Phone\", \"category\": \"Electronics\", \"price\": 800}\n    ],\n    \"Clothing\": [\n      {\"name\": \"Shirt\", \"category\": \"Clothing\", \"price\": 50},\n      {\"name\": \"Jeans\", \"category\": \"Clothing\", \"price\": 80}\n    ]\n  }\n}",
        "name": "inventory"
      }
    ],
    "scriptText": "[\r\n  {\r\n    \"path\": \"$.inventory.grouped\",\r\n    \"source\": \"$.inventory.products\",\r\n    \"groupBy\": \"category\",\r\n    \"command\": \"group\"\r\n  }\r\n]"
