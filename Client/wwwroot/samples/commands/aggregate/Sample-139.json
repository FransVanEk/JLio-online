{
  "description": "This sample shows calculating average, min, and max values from numeric arrays.",
  "model": {
    "inputObjects": [
      {
        "jsonString": "{\r\n  \"scores\": [\r\n    {\"student\": \"Alice\", \"score\": 85},\r\n    {\"student\": \"Bob\", \"score\": 92},\r\n    {\"student\": \"Charlie\", \"score\": 78},\r\n    {\"student\": \"David\", \"score\": 95}\r\n  ]\r\n}",
        "name": "results"
      }
    ],
    "name": "Sample-139",
    "OutputObjects": [
      {
        "jsonString": "{\n  \"scores\": [\n    {\"student\": \"Alice\", \"score\": 85},\n    {\"student\": \"Bob\", \"score\": 92},\n    {\"student\": \"Charlie\", \"score\": 78},\n    {\"student\": \"David\", \"score\": 95}\n  ],\n  \"statistics\": {\n    \"average\": 87.5,\n    \"min\": 78,\n    \"max\": 95\n  }\n}",
        "name": "results"
      }
    ],
    "scriptText": "[\r\n  {\r\n    \"path\": \"$.results.statistics\",\r\n    \"source\": \"$.results.scores\",\r\n    \"aggregations\": [\r\n      {\"field\": \"score\", \"function\": \"average\", \"as\": \"average\"},\r\n      {\"field\": \"score\", \"function\": \"min\", \"as\": \"min\"},\r\n      {\"field\": \"score\", \"function\": \"max\", \"as\": \"max\"}\r\n    ],\r\n    \"command\": \"aggregate\"\r\n  }\r\n]"
  },
  "tags": [
    "aggregate",
    "etl"
  ],
  "title": "Calculate statistics (avg, min, max)"
}