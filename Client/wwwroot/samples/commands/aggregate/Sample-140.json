{
  "description": "This sample demonstrates aggregating distinct values and creating unique lists.",
  "model": {
    "inputObjects": [
      {
        "jsonString": "{\r\n  \"orders\": [\r\n    {\"customer\": \"Alice\", \"city\": \"New York\"},\r\n    {\"customer\": \"Bob\", \"city\": \"Boston\"},\r\n    {\"customer\": \"Charlie\", \"city\": \"New York\"},\r\n    {\"customer\": \"David\", \"city\": \"Chicago\"}\r\n  ]\r\n}",
        "name": "data"
      }
    ],
    "name": "Sample-140",
    "OutputObjects": [
      {
        "jsonString": "{\n  \"orders\": [\n    {\"customer\": \"Alice\", \"city\": \"New York\"},\n    {\"customer\": \"Bob\", \"city\": \"Boston\"},\n    {\"customer\": \"Charlie\", \"city\": \"New York\"},\n    {\"customer\": \"David\", \"city\": \"Chicago\"}\n  ],\n  \"uniqueCities\": [\"New York\", \"Boston\", \"Chicago\"],\n  \"cityCount\": 3\n}",
        "name": "data"
      }
    ],
    "scriptText": "[\r\n  {\r\n    \"path\": \"$.data.uniqueCities\",\r\n    \"source\": \"$.data.orders\",\r\n    \"field\": \"city\",\r\n    \"function\": \"distinct\",\r\n    \"command\": \"aggregate\"\r\n  },\r\n  {\r\n    \"path\": \"$.data.cityCount\",\r\n    \"value\": \"=count(@.uniqueCities)\",\r\n    \"command\": \"add\"\r\n  }\r\n]"
  },
