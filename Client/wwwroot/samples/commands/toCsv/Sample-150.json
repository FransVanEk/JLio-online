                                           "name":  "data"
                                       }
                                   ],
                  "outputObjects":  [
                                        {
                                            "jsonString":  "{\r\n  \"orders\": [\r\n    {\r\n      \"orderId\": \"ORD001\",\r\n      \"amount\": 150.5\r\n    },\r\n    {\r\n      \"orderId\": \"ORD002\",\r\n      \"amount\": 200.0\r\n    }\r\n  ],\r\n  \"report\": \"orderId,amount\\nORD001,150.5\\nORD002,200\"\r\n}",
