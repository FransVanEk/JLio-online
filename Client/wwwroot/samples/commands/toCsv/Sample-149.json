                                            "jsonString":  "{\r\n  \"products\": [\r\n    {\r\n      \"sku\": \"A123\",\r\n      \"name\": \"Laptop\",\r\n      \"price\": 999\r\n    },\r\n    {\r\n      \"sku\": \"B456\",\r\n      \"name\": \"Mouse\",\r\n      \"price\": 25\r\n    }\r\n  ],\r\n  \"export\": \"sku;name;price\\nA123;Laptop;999\\nB456;Mouse;25\"\r\n}",
