{
  "title": "Resolve array of references",
  "tags": [
    "resolve"
  ],
  "model": {
    "name": "Sample-141",
    "scriptText": "[\r\n  {\r\n    \"path\": \"$.tags[*]\",\r\n    \"refPath\": \"$.tagStore\",\r\n    \"command\": \"resolve\"\r\n  }\r\n]",
    "inputObjects": [
      {
        "tags": [
          "ref://tagStore/tag1",
          "ref://tagStore/tag2"
        ],
        "tagStore": {
          "tag1": "JavaScript",
          "tag2": "Python"
        }
      }
    ],
    "outputObjects": [
      {
        "tags": [
          "JavaScript",
          "Python"
        ],
        "tagStore": {
          "tag1": "JavaScript",
          "tag2": "Python"
        }
      }
    ]
  },
  "description": "This sample shows resolve array of references."
}