          },
          "200": {
            "name": "Bob"
          }
        }
      }
    ],
    "outputObjects": [
      {
        "userId": {
          "name": "Alice"
        },
        "managerId": {
          "name": "Bob"
        },
        "users": {
          "100": {
            "name": "Alice"
          },
          "200": {
            "name": "Bob"
          }
        }
      }
    ]
  },
