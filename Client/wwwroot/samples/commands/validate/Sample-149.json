        "jsonString": "{\n  \"records\": [\n    {\"age\": 25, \"email\": \"valid@example.com\", \"valid\": true},\n    {\"age\": \"thirty\", \"email\": \"invalid-email\", \"valid\": false},\n    {\"age\": 35, \"email\": \"another@example.com\", \"valid\": true}\n  ]\n}",
