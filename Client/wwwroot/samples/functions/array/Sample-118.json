{
  "description": "This sample demonstrates using array count function to get the number of elements in arrays.",
  "model": {
    "inputObjects": [
      {
        "jsonString": "{\r\n  \"items\": [1, 2, 3, 4, 5],\r\n  \"tags\": [\"a\", \"b\", \"c\"]\r\n}",
        "name": "data"
      }
    ],
    "name": "Sample-118",
    "OutputObjects": [
      {
        "jsonString": "{\n  \"items\": [1, 2, 3, 4, 5],\n  \"tags\": [\"a\", \"b\", \"c\"],\n  \"itemCount\": 5,\n  \"tagCount\": 3\n}",
        "name": "data"
      }
    ],
    "scriptText": "[\r\n  {\r\n    \"path\": \"$.data.itemCount\",\r\n    \"value\": \"=count(@.items)\",\r\n    \"command\": \"add\"\r\n  },\r\n  {\r\n    \"path\": \"$.data.tagCount\",\r\n    \"value\": \"=count(@.tags)\",\r\n    \"command\": \"add\"\r\n  }\r\n]"
  },
  "tags": [
    "add",
    "array"
  ],
  "title": "Count array elements"
}