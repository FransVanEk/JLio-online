{
  "description": "This sample shows using indexOf to find element positions in arrays and strings.",
  "model": {
    "inputObjects": [
      {
        "jsonString": "{\r\n  \"text\": \"Hello World\",\r\n  \"items\": [\"apple\", \"banana\", \"cherry\"]\r\n}",
        "name": "data"
      }
    ],
    "name": "Sample-121",
    "OutputObjects": [
      {
        "jsonString": "{\n  \"text\": \"Hello World\",\n  \"items\": [\"apple\", \"banana\", \"cherry\"],\n  \"worldIndex\": 6,\n  \"bananaIndex\": 1\n}",
        "name": "data"
      }
    ],
    "scriptText": "[\r\n  {\r\n    \"path\": \"$.data.worldIndex\",\r\n    \"value\": \"=indexOf(@.text,'World')\",\r\n    \"command\": \"add\"\r\n  },\r\n  {\r\n    \"path\": \"$.data.bananaIndex\",\r\n    \"value\": \"=indexOf(@.items,'banana')\",\r\n    \"command\": \"add\"\r\n  }\r\n]"
  },
  "tags": [
    "add",
    "array"
  ],
  "title": "Find index of elements"
}