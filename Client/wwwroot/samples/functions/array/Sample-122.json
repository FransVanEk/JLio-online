{
  "description": "This sample demonstrates checking if strings start with or end with specific patterns.",
  "model": {
    "inputObjects": [
      {
        "jsonString": "{\r\n  \"file1\": \"document.pdf\",\r\n  \"file2\": \"image.png\",\r\n  \"url\": \"https://example.com\"\r\n}",
        "name": "data"
      }
    ],
    "name": "Sample-122",
    "OutputObjects": [
      {
        "jsonString": "{\n  \"file1\": \"document.pdf\",\n  \"file2\": \"image.png\",\n  \"url\": \"https://example.com\",\n  \"isPdf\": true,\n  \"isPng\": true,\n  \"isSecure\": true\n}",
        "name": "data"
      }
    ],
    "scriptText": "[\r\n  {\r\n    \"path\": \"$.data.isPdf\",\r\n    \"value\": \"=endsWith(@.file1,'.pdf')\",\r\n    \"command\": \"add\"\r\n  },\r\n  {\r\n    \"path\": \"$.data.isPng\",\r\n    \"value\": \"=endsWith(@.file2,'.png')\",\r\n    \"command\": \"add\"\r\n  },\r\n  {\r\n    \"path\": \"$.data.isSecure\",\r\n    \"value\": \"=startsWith(@.url,'https')\",\r\n    \"command\": \"add\"\r\n  }\r\n]"
  },
  "tags": [
    "add",
    "array"
  ],
  "title": "Check string patterns with startsWith and endsWith"
}