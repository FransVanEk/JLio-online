    "scriptText": "[\r\n  {\r\n    \"path\": \"$.task.isInactive\",\r\n    \"value\": \"=not(@.isActive)\",\r\n    \"command\": \"add\"\r\n  },\r\n  {\r\n    \"path\": \"$.task.isCompleted\",\r\n    \"value\": \"=equals(@.status,'completed')\",\r\n    \"command\": \"add\"\r\n  }\r\n]"
