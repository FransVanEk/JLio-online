{
  "description": "This sample shows using if-then-else logic for conditional value assignment.",
  "model": {
    "inputObjects": [
      {
        "jsonString": "{\r\n  \"score\": 75,\r\n  \"temperature\": 22\r\n}",
        "name": "data"
      }
    ],
    "name": "Sample-126",
    "OutputObjects": [
      {
        "jsonString": "{\n  \"score\": 75,\n  \"temperature\": 22,\n  \"grade\": \"C\",\n  \"comfort\": \"comfortable\"\n}",
        "name": "data"
      }
    ],
    "scriptText": "[\r\n  {\r\n    \"path\": \"$.data.grade\",\r\n    \"value\": \"=if(greaterThan(@.score,90),'A',if(greaterThan(@.score,80),'B','C'))\",\r\n    \"command\": \"add\"\r\n  },\r\n  {\r\n    \"path\": \"$.data.comfort\",\r\n    \"value\": \"=if(greaterThan(@.temperature,25),'hot','comfortable')\",\r\n    \"command\": \"add\"\r\n  }\r\n]"
  },
