{
  "description": "This sample demonstrates comparison functions like greaterThan, lessThan for numeric comparisons.",
  "model": {
    "inputObjects": [
      {
        "jsonString": "{\r\n  \"price\": 99.99,\r\n  \"budget\": 100,\r\n  \"quantity\": 5,\r\n  \"minOrder\": 3\r\n}",
        "name": "order"
      }
    ],
    "name": "Sample-125",
    "OutputObjects": [
      {
        "jsonString": "{\n  \"price\": 99.99,\n  \"budget\": 100,\n  \"quantity\": 5,\n  \"minOrder\": 3,\n  \"withinBudget\": true,\n  \"meetsMinimum\": true\n}",
        "name": "order"
      }
    ],
    "scriptText": "[\r\n  {\r\n    \"path\": \"$.order.withinBudget\",\r\n    \"value\": \"=lessThanOrEqual(@.price,@.budget)\",\r\n    \"command\": \"add\"\r\n  },\r\n  {\r\n    \"path\": \"$.order.meetsMinimum\",\r\n    \"value\": \"=greaterThanOrEqual(@.quantity,@.minOrder)\",\r\n    \"command\": \"add\"\r\n  }\r\n]"
  },
