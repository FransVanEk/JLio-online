{
  "description": "This sample demonstrates using isNull and isEmpty checks for validation.",
  "model": {
    "inputObjects": [
      {
        "jsonString": "{\r\n  \"value1\": null,\r\n  \"value2\": \"\",\r\n  \"value3\": \"data\"\r\n}",
        "name": "fields"
      }
    ],
    "name": "Sample-127",
    "OutputObjects": [
      {
        "jsonString": "{\n  \"value1\": null,\n  \"value2\": \"\",\n  \"value3\": \"data\",\n  \"check1\": true,\n  \"check2\": true,\n  \"check3\": false\n}",
        "name": "fields"
      }
    ],
    "scriptText": "[\r\n  {\r\n    \"path\": \"$.fields.check1\",\r\n    \"value\": \"=isNull(@.value1)\",\r\n    \"command\": \"add\"\r\n  },\r\n  {\r\n    \"path\": \"$.fields.check2\",\r\n    \"value\": \"=isEmpty(@.value2)\",\r\n    \"command\": \"add\"\r\n  },\r\n  {\r\n    \"path\": \"$.fields.check3\",\r\n    \"value\": \"=isEmpty(@.value3)\",\r\n    \"command\": \"add\"\r\n  }\r\n]"
  },
  "tags": [
    "add",
    "logic"
  ],
  "title": "Null and empty checks"
}