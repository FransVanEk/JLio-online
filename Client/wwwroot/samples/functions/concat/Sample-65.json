        "jsonString": "{\n  \"protocol\": \"https\",\n  \"domain\": \"api.example.com\",\n  \"path\": \"/users\",\n  \"id\": 123,\n  \"url\": \"https://api.example.com/users/123\"\n}",
        "name": "endpoint"
      }
    ],
    "scriptText": "[\r\n  {\r\n    \"path\": \"$.endpoint.url\",\r\n    \"value\": \"=concat(@.protocol,'://',@.domain,@.path,'/',@.id)\",\r\n    \"command\": \"add\"\r\n  }\r\n]"
  },
