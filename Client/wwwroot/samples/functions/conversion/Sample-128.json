{
  "description": "This sample demonstrates converting strings to numbers with different numeric types.",
  "model": {
    "inputObjects": [
      {
        "jsonString": "{\r\n  \"intString\": \"42\",\r\n  \"floatString\": \"3.14\",\r\n  \"hexString\": \"FF\"\r\n}",
        "name": "strings"
      }
    ],
    "name": "Sample-128",
    "OutputObjects": [
      {
        "jsonString": "{\n  \"intString\": \"42\",\n  \"floatString\": \"3.14\",\n  \"hexString\": \"FF\",\n  \"integer\": 42,\n  \"decimal\": 3.14,\n  \"hex\": 255\n}",
        "name": "strings"
      }
    ],
    "scriptText": "[\r\n  {\r\n    \"path\": \"$.strings.integer\",\r\n    \"value\": \"=toInt(@.intString)\",\r\n    \"command\": \"add\"\r\n  },\r\n  {\r\n    \"path\": \"$.strings.decimal\",\r\n    \"value\": \"=toDecimal(@.floatString)\",\r\n    \"command\": \"add\"\r\n  },\r\n  {\r\n    \"path\": \"$.strings.hex\",\r\n    \"value\": \"=toInt(@.hexString,16)\",\r\n    \"command\": \"add\"\r\n  }\r\n]"
  },
