      }
    ],
    "scriptText": "[\r\n  {\r\n    \"path\": \"$.credentials.passwordLength\",\r\n    \"value\": \"=length(@.password)\",\r\n    \"command\": \"add\"\r\n  },\r\n  {\r\n    \"path\": \"$.credentials.isValidEmail\",\r\n    \"value\": \"=contains(@.email,'@')\",\r\n    \"command\": \"add\"\r\n  }\r\n]"
