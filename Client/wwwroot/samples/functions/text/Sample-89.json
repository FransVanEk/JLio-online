    "OutputObjects": [
      {
        "jsonString": "{\n  \"text\": \"HelloWorld\",\n  \"code\": \"AB-CD-EF\",\n  \"insertedText\": \"Hello World\",\n  \"cleanCode\": \"ABCDEF\"\n}",
        "name": "data"
      }
    ],
    "scriptText": "[\r\n  {\r\n    \"path\": \"$.data.insertedText\",\r\n    \"value\": \"=insert(@.text,5,' ')\",\r\n    \"command\": \"add\"\r\n  },\r\n  {\r\n    \"path\": \"$.data.cleanCode\",\r\n    \"value\": \"=delete(@.code,'-')\",\r\n    \"command\": \"add\"\r\n  }\r\n]"
  },
