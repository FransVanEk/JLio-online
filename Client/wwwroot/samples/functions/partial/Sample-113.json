{
  "description": "This sample shows using partial for creating lightweight copies of large objects with only essential fields.",
  "model": {
    "inputObjects": [
      {
        "jsonString": "[\r\n  {\"id\": 1, \"title\": \"Task 1\", \"description\": \"Long description...\", \"metadata\": {}, \"history\": []},\r\n  {\"id\": 2, \"title\": \"Task 2\", \"description\": \"Another long...\", \"metadata\": {}, \"history\": []}\r\n]",
        "name": "tasks"
      }
    ],
    "name": "Sample-113",
    "outputObjects": [
      {
        "jsonString": "[\r\n  {\"id\": 1, \"title\": \"Task 1\"},\r\n  {\"id\": 2, \"title\": \"Task 2\"}\r\n]",
        "name": "tasks"
      }
    ],
    "scriptText": "[\r\n  {\r\n    \"path\": \"$.tasks[*]\",\r\n    \"value\": \"=partial(@.id,@.title)\",\r\n    \"command\": \"set\"\r\n  }\r\n]"
  },
  "tags": [
    "set",
    "partial"
  ],
  "title": "Partial for lightweight object copies"
