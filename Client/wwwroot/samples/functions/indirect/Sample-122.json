{
  "title": "Indirect with nested object navigation",
  "tags": ["indirect"],
  "inputObjects": [
    {
      "targetPath": "order.customer.email",
      "order": {
        "id": 123,
        "customer": {
          "name": "Alice",
          "email": "alice@example.com"
        }
      }
    }
  ],
  "scriptText": "[\r\n  {\r\n    \"path\": \"$.contactEmail\",\r\n    \"value\": \"=indirect($.targetPath)\",\r\n    \"command\": \"add\"\r\n  }\r\n]",
  "OutputObjects": [
    {
      "targetPath": "order.customer.email",
      "order": {
        "id": 123,
        "customer": {
          "name": "Alice",
          "email": "alice@example.com"
        }
      },
      "contactEmail": "alice@example.com"
    }
  ]
}
