{
  "title": "Indirect reference to dynamic path",
  "tags": [
    "indirect"
  ],
  "model": {
    "name": "Sample-118",
    "scriptText": "[\r\n  {\r\n    \"path\": \"$.result\",\r\n    \"value\": \"=indirect($.dynamicField)\",\r\n    \"command\": \"add\"\r\n  }\r\n]",
    "inputObjects": [
      {
        "jsonString": "{\r\n  \"dynamicField\": \"user.name\",\r\n  \"user\": {\r\n    \"name\": \"John Doe\"\r\n  }\r\n}",
        "name": "data"
      }
    ],
    "outputObjects": [
      {
        "jsonString": "{\r\n  \"dynamicField\": \"user.name\",\r\n  \"user\": {\r\n    \"name\": \"John Doe\"\r\n  },\r\n  \"result\": \"John Doe\"\r\n}",
        "name": "data"
      }
    ]
  },
  "description": "This sample shows indirect reference to dynamic path."
}