{
  "title": "Indirect array element access",
  "tags": [
    "indirect"
  ],
  "model": {
    "name": "Sample-121",
    "scriptText": "[\r\n  {\r\n    \"path\": \"$.selectedColor\",\r\n    \"value\": \"=indirect($.arrayPath)\",\r\n    \"command\": \"add\"\r\n  }\r\n]",
    "inputObjects": [
      {
        "arrayPath": "colors[2]",
        "colors": [
          "red",
          "blue",
          "green",
          "yellow"
        ]
      }
    ],
    "outputObjects": [
      {
        "arrayPath": "colors[2]",
        "colors": [
          "red",
          "blue",
          "green",
          "yellow"
        ],
        "selectedColor": "green"
      }
    ]
  },
  "description": "This sample shows indirect array element access."
}