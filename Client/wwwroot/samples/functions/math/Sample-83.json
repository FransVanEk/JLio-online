{
  "description": "This sample demonstrates using abs, round, and ceil functions for number manipulation.",
  "model": {
    "inputObjects": [
      {
        "jsonString": "{\r\n  \"value\": -15.67,\r\n  \"price\": 19.99\r\n}",
        "name": "data"
