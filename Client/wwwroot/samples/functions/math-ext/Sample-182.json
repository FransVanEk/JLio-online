{
    "title":  "Sum - Total array values",
    "tags":  [
                 "add",
                 "sum"
             ],
    "model":  {
                  "name":  "Sample-182",
                  "scriptText":  "[\r\n  {\r\n    \"path\": \"$.totalAmount\",\r\n    \"value\": \"=sum(@.amounts[*])\",\r\n    \"command\": \"add\"\r\n  }\r\n]",
                  "inputObjects":  [
                                       {
                                           "jsonString":  "{\\\"amounts\\\":[100,250,75,125]}",
                                           "name":  "t"
                                       }
                                   ],
                  "outputObjects":  [
                                        {
                                            "jsonString":  "{\\\"amounts\\\":[100,250,75,125],\\\"totalAmount\\\":550}",
                                            "name":  "t"
                                        }
                                    ]
              },
    "description":  "This sample shows sum - total array values."
}
