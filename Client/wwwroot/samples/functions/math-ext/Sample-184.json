{
  "title": "Subtract - Subtract two values",
  "tags": ["add", "subtract"],
  "inputObjects": [
    {
      "total": 500,
      "discount": 75
    }
  ],
  "scriptText": "[\r\n  {\r\n    \"path\": \"$.finalPrice\",\r\n    \"value\": \"=subtract(@.total, @.discount)\",\r\n    \"command\": \"add\"\r\n  }\r\n]",
  "outputObjects": [
    {
      "total": 500,
      "discount": 75,
      "finalPrice": 425
    }
  ]
}