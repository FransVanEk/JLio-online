{
  "description": "This sample shows using dayofweek and weekofyear functions to extract calendar information from dates.",
  "model": {
    "inputObjects": [
      {
        "jsonString": "{\r\n  \"date\": \"2024-03-15T00:00:00Z\"\r\n}",
        "name": "calendar"
      }
    ],
    "name": "Sample-95",
    "OutputObjects": [
      {
        "jsonString": "{\n  \"date\": \"2024-03-15T00:00:00Z\",\n  \"dayOfWeek\": \"Friday\",\n  \"weekNumber\": 11,\n  \"isWeekend\": false\n}",
        "name": "calendar"
      }
    ],
    "scriptText": "[\r\n  {\r\n    \"path\": \"$.calendar.dayOfWeek\",\r\n    \"value\": \"=dayofweek(@.date)\",\r\n    \"command\": \"add\"\r\n  },\r\n  {\r\n    \"path\": \"$.calendar.weekNumber\",\r\n    \"value\": \"=weekofyear(@.date)\",\r\n    \"command\": \"add\"\r\n  },\r\n  {\r\n    \"path\": \"$.calendar.isWeekend\",\r\n    \"value\": \"=isweekend(@.date)\",\r\n    \"command\": \"add\"\r\n  }\r\n]"
  },
  "tags": [
    "add",
    "timedate"
  ],
  "title": "Day of week and calendar functions"
}