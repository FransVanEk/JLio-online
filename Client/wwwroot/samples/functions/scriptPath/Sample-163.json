{
    "title": "ScriptPath - Track array item paths",
    "tags": [
        "add",
        "scriptPath"
    ],
    "model": {
        "name": "Sample-163",
        "scriptText": "[\r\n  {\r\n    \"path\": \"$.data.customers[*].idPath\",\r\n    \"value\": \"=scriptPath()\",\r\n    \"command\": \"add\"\r\n  }\r\n]",
        "inputObjects": [
            {
                "jsonString": "{\"customers\":[{\"id\":\"C001\",\"name\":\"Alice\"},{\"id\":\"C002\",\"name\":\"Bob\"}]}",
                "name": "data"
            }
        ],
        "outputObjects": [
            {
                "jsonString": "{\"customers\":[{\"id\":\"C001\",\"name\":\"Alice\",\"idPath\":\"$.data.customers[0]\"},{\"id\":\"C002\",\"name\":\"Bob\",\"idPath\":\"$.data.customers[1]\"}]}",
                "name": "data"
            }
        ]
    },
    "description": "This sample shows scriptpath - track array item paths."
}
